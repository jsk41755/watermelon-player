CREATE TABLE historyEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    musicName TEXT NOT NULL,
    timestamp INTEGER NOT NULL
);

getHistoryList:
SELECT *
FROM historyEntity
ORDER BY timestamp ASC;

insertHistoryInstance:
INSERT OR REPLACE
INTO historyEntity(
    id,
    musicName,
    timestamp
)
VALUES(?, ?,?);

deleteHistoryInstance:
DELETE FROM historyEntity
WHERE musicName = ?;